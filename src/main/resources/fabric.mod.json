{
	"schemaVersion": 1,
	"id": "reforested",
	"version": "${version}",
	"name": "Reforested",
	"description": "A mod all about forestry",
	"authors": [
		"Acrogenous"
	],
	"accessWidener" : "poi.accesswidener",
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/reforested/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ace.actually.reforested.Reforested"
		],
		"fabric-datagen": [
			"ace.actually.reforested.ReforestedDataGenerator"
		],
		"client": [
			"ace.actually.reforested.ClientInit"
		],
		"mm:early_risers": [
			{
				"value": "ace.actually.reforested.RisingEarly"
			}
		]
	},
	"mixins": [
		"reforested.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}